            ELEMENT[id3.1] matches {    -- Agent
                value matches {
                    DV_TEXT[id15.1] 
                }
            }
            ELEMENT[id4.1] matches {    -- Overall Description
                value matches {
                    DV_TEXT[id16.1] 
                }
            }
            ELEMENT[id6.1] matches {    -- Category
                value matches {
                    DV_CODED_TEXT[id17.1] matches {
                        defining_code matches {[ac1]}
                    }
                }
            }
            ELEMENT[id5.1] matches {    -- Ongoing Exposure
                value matches {
                    DV_BOOLEAN[id18.1] matches {
                        value matches {True}
                    }
