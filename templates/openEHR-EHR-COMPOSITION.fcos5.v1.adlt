                    ELEMENT[id12.1] matches {    -- Date of Onset
                        value matches {
                            DV_DATE_TIME[id19.1] 
                        }
                    }
                    ELEMENT[id14.1] matches {    -- Description
                        value matches {
                            DV_TEXT[id20.1] 
                        }
                    }
                    ELEMENT[id13.1] matches {    -- Date Ceased
                        value matches {
                            DV_DATE_TIME[id21.1] 
                        }
                    }
                }
            }
        }
    }

