                    ELEMENT[id13.1] matches {    -- Date Ceased
                        value matches {
                            DV_DATE_TIME[id21.1] 
                        }
                    }
                }
            }
        }
        /protocol[id9]/items matches {
            ELEMENT[id10.1] matches {    -- Date Updated
                value matches {
                    DV_DATE_TIME[id22.1] 
                }
            }
        }
    }

terminology
